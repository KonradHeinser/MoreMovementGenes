  <GeneDef>
    <defName>MBSG_SuperNakedSpeed</defName>
    <label>super naked speed</label>
    <description>Carriers of this gene move much slower while clothed, and faster while naked.</description>
    <iconPath>UI/Icons/Genes/Gene_NakedSpeed</iconPath>
    <displayCategory>Movement</displayCategory>
    <displayOrderInCategory>500.4</displayOrderInCategory>
    <conditionalStatAffecters>
      <li Class="ConditionalStatAffecter_Clothed">
        <statOffsets>
          <MoveSpeed>-0.6</MoveSpeed>
        </statOffsets>
      </li>
      <li Class="ConditionalStatAffecter_Unclothed">
        <statOffsets>
          <MoveSpeed>0.3</MoveSpeed>
        </statOffsets>
      </li>
    </conditionalStatAffecters>
    <biostatCpx>3</biostatCpx>
    <biostatMet>6</biostatMet>
  </GeneDef>

